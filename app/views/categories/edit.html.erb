<%= form_for(@category)do |f| %>
  <div class="col-md-12">
    <div class="row">
      <div class="col-md-6">
        <div class="form-group">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>
      </div>

      <div class="col-md-6">
      </div>
      <%= f.submit "Update", class: "btn btn-primary ml-3" %>
    </div>
  </div>
<% end %>
